using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication2
{
    class Program
    {
        static void Main(string[] args)
        {
            int istrue = 8;
            if (istrue <= 80)
                Console.WriteLine("it it true");
            else
                Console.WriteLine("it is false");
        

        }
    }
}


